/* pages/personal/extension.wxss */
.img-box {
  margin: 8px auto;
  position: relative;
  width: 500rpx;
  height: 500rpx;
  background-color: #fff;
}

.img-preview {
  width: 100%;
  height: 100%;
}

.canvas-box {
  position: fixed;
  top: -9999px;
  visibility: hidden;
}

.canvas-box canvas {
  width: 300px;
  height: 300px;
}